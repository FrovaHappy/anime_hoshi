{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "env-cmd": "10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.14.2",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "lint": "ts-standard --fix",
    "dev:local": "env-cmd -e LOCAL react-scripts start",
    "dev:cloud": "env-cmd -e CLOUD react-scripts start",
    "build": "env-cmd -e CLOUD react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "eslint": "8.45.0",
    "eslint-config-standard-with-typescript": "36.1.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "16.0.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.0",
    "sass": "1.64.0",
    "ts-standard": "12.0.2",
    "typescript": "5.1.6"
  }
}
